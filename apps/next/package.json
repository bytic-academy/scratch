{
  "name": "@acme/next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "pnpm with-env prisma migrate dev",
    "db:migrate": "pnpm with-env prisma migrate deploy",
    "db:push": "pnpm with-env prisma db push",
    "db:studio": "pnpm with-env prisma studio",
    "preview": "next build && next start",
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm with-env next dev --turbopack",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "postinstall": "pnpm with-env prisma generate"
  },
  "dependencies": {
    "@acme/packager": "workspace:^",
    "@auth/prisma-adapter": "^2.7.2",
    "@hookform/resolvers": "^5.1.0",
    "@prisma/client": "^6.16.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.6",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "^15.3.3",
    "next-auth": "5.0.0-beta.25",
    "postcss": "^8.5.6",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-hook-form": "^7.57.0",
    "server-only": "^0.0.1",
    "superjson": "2.2.2",
    "tailwind-merge": "^3.3.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@biomejs/biome": "1.9.4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.29",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.16.1",
    "tailwindcss": "catalog:",
    "tw-animate-css": "^1.3.8",
    "typescript": "catalog:"
  },
  "prettier": "@acme/prettier-config"
}